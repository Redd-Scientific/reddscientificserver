<table>
    <thead>
        <tr>
            <th>Question</th>
            <th>Answer A</th>
            <th>Answer B</th>
            <th>Answer C</th>
            <th>Answer D</th>
        </tr>
    </thead>
    <tbody data-bind="foreach: questions">
        <tr>    
            <td data-bind="text: Question1"></td>
            <td data-bind="text: AnswerA"></td>
            <td data-bind="text: AnswerB"></td>
            <td data-bind="text: AnswerC"></td>
            <td data-bind="text: AnswerD"></td>
        </tr>
    </tbody>
</table>
<input type="button" id="btnGetCustomers" value="Get Questions" data-bind="click: getQuestions" />